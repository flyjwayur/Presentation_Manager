    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.3.15",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.8",
    "package.json": "^2.0.1",
    "validator": "^10.10.0"
  }
